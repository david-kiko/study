{
    "author": "David Li <david@linktime.cloud>",
    "description": "This is a sample book created by gitbook",
    "extension": null,
    "generator": "site",
    "links": {
        "sharing": {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null
        }
    },
    "output": null,
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "tbfed-pagefooter",
        "-lunr", 
        "-search", 
        "search-plus",
        "theme-comscore",
        "theme-api",
        "expandable-chapters",
        "splitter",
        "back-to-top-button",
        "edit-link",
        "prism",
        "-highlight",
        "copy-code-button",
        "-sharing",
        "pageview-count",
        "click-heart"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
            "copyright":"&copy david",
            "modify_label": "文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "theme-default": {
            "showLevel": true
        },
        "theme-api": {
            "theme": "dark"
        },
        "edit-link": {
            "base": "https://github.com/david-kiko/study/edit/main/-",
            "label": "编辑此页面"
       }
    },
    "styles": {
        "website": "styles/website.css"
    },
    "title": "Sample GitBook"
}